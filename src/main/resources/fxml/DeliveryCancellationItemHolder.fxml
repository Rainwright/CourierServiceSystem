<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox minWidth="600.0" prefHeight="70.0" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" minWidth="150.0">
         <children>
            <Label fx:id="lbl_one" text="Label">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" minWidth="150.0">
         <children>
            <Label fx:id="lbl_two" text="Label">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="lbl_three" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Label>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </HBox>
      <HBox alignment="CENTER" minWidth="150.0">
         <children>
            <Label fx:id="lbl_four" text="Label">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="btn_one" mnemonicParsing="false" prefHeight="25.0" prefWidth="35.0" style="-fx-background-color: lime;" text="Yes">
               <HBox.margin>
                  <Insets right="25.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btn_two" mnemonicParsing="false" prefHeight="25.0" prefWidth="35.0" style="-fx-background-color: red;" text="No" />
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
</HBox>
