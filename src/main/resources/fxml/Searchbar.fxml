               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <children>
                  <TextField fx:id="search_text_field" promptText="ui_search_label" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <font>
                        <Font name="Arial" size="22.0" />
                     </font>
                  </TextField>
               </children>
            </AnchorPane>
         </children>
